<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans.xsd
	   http://www.eclipse.org/gemini/blueprint/schema/blueprint
	   http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">

	<bean
		id="com.liferay.test.blueprint.spring.CustomUserLocalService"
		class="com.liferay.test.blueprint.spring.CustomUserLocalService"
	/>

	<osgi:service
		id="customUserLocalService"
		ref="com.liferay.test.blueprint.spring.CustomUserLocalService"
		interface="com.liferay.portal.service.UserLocalService"
	>
		<osgi:service-properties>
			<entry key="portal.service">
				<value type="java.lang.Boolean">true</value>
			</entry>
			<entry key="portal.service.wrapper">
				<value type="java.lang.Boolean">true</value>
			</entry>
			<entry
				key="portal.service.type"
				value="com.liferay.portal.service.UserLocalService"
			/>
		</osgi:service-properties>
	</osgi:service>
</beans>