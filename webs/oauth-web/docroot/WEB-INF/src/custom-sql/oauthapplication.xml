<?xml version="1.0"?>

<custom-sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.countAll">
		<![CDATA[
			SELECT
				COUNT(*) AS COUNT_VALUE
			FROM
				OAuthApplication
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.countByName">
		<![CDATA[
			SELECT
				COUNT(*) AS COUNT_VALUE
			FROM
				OAuthApplication
			WHERE
				(name LIKE ?)
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.countByN_O">
		<![CDATA[
			SELECT
				COUNT(*) AS COUNT_VALUE
			FROM
				OAuthApplication
			WHERE
				(name LIKE ?) AND
				(ownerId = ?)
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.countByWebsite">
		<![CDATA[
			SELECT
				COUNT(*) AS COUNT_VALUE
			FROM
				OAuthApplication
			WHERE
				(name LIKE ?)
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.findAll">
		<![CDATA[
			SELECT
				*
			FROM
				OAuthApplication
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.findByName">
		<![CDATA[
			SELECT
				*
			FROM
				OAuthApplication
			WHERE
				(name LIKE ?)
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.findByN_O">
		<![CDATA[
			SELECT
				*
			FROM
				OAuthApplication
			WHERE
				(name LIKE ?) AND
				(ownerId = ?)
		]]>
	</sql>
	<sql id="com.liferay.portal.oauth.service.persistence.OAuthApplicationFinder.findByWebsite">
		<![CDATA[
			SELECT
				*
			FROM
				OAuthApplication
			WHERE
				(website LIKE ?)
		]]>
	</sql>
</custom-sql>