<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.3.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_3_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<header-portlet-css>/summary/css.jsp</header-portlet-css>
		<css-class-wrapper>wol-portlet-summary</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<icon>/icons/svn.png</icon>
		<scheduler-class>com.liferay.wol.svn.job.SVNScheduler</scheduler-class>
		<friendly-url-mapper-class>com.liferay.wol.svn.portlet.SVNFriendlyURLMapper</friendly-url-mapper-class>
		<social-activity-interpreter-class>com.liferay.wol.svn.social.SVNActivityInterpreter</social-activity-interpreter-class>
		<header-portlet-css>/svn/css.jsp</header-portlet-css>
		<css-class-wrapper>wol-portlet-svn</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>3</portlet-name>
		<icon>/icons/jira.png</icon>
		<scheduler-class>com.liferay.wol.jira.job.JIRAScheduler</scheduler-class>
		<social-activity-interpreter-class>com.liferay.wol.jira.social.JIRAActivityInterpreter</social-activity-interpreter-class>
		<header-portlet-css>/jira/css.jsp</header-portlet-css>
		<css-class-wrapper>wol-portlet-jira</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>4</portlet-name>
		<icon>/icons/friends.png</icon>
		<social-activity-interpreter-class>com.liferay.wol.friends.social.FriendsActivityInterpreter</social-activity-interpreter-class>
		<social-request-interpreter-class>com.liferay.wol.friends.social.FriendsRequestInterpreter</social-request-interpreter-class>
		<header-portlet-css>/friends/css.jsp</header-portlet-css>
		<css-class-wrapper>wol-portlet-friends</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>5</portlet-name>
		<icon>/icons/wall.png</icon>
		<friendly-url-mapper-class>com.liferay.wol.wall.portlet.WallFriendlyURLMapper</friendly-url-mapper-class>
		<social-activity-interpreter-class>com.liferay.wol.wall.social.WallActivityInterpreter</social-activity-interpreter-class>
		<restore-current-view>false</restore-current-view>
		<css-class-wrapper>wol-portlet-wall</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>6</portlet-name>
		<friendly-url-mapper-class>com.liferay.wol.friendsactivities.portlet.FriendsActivitiesFriendlyURLMapper</friendly-url-mapper-class>
		<css-class-wrapper>wol-portlet-friends-activities</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>7</portlet-name>
		<icon>/icons/members.png</icon>
		<social-activity-interpreter-class>com.liferay.wol.members.social.MembersActivityInterpreter</social-activity-interpreter-class>
		<social-request-interpreter-class>com.liferay.wol.members.social.MembersRequestInterpreter</social-request-interpreter-class>
		<header-portlet-css>/members/css.jsp</header-portlet-css>
		<css-class-wrapper>wol-portlet-members</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>8</portlet-name>
		<friendly-url-mapper-class>com.liferay.wol.map.portlet.MapFriendlyURLMapper</friendly-url-mapper-class>
		<ajaxable>false</ajaxable>
		<css-class-wrapper>wol-portlet-map</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>9</portlet-name>
		<friendly-url-mapper-class>com.liferay.wol.meetups.portlet.MeetupsFriendlyURLMapper</friendly-url-mapper-class>
		<restore-current-view>false</restore-current-view>
		<css-class-wrapper>wol-portlet-meetups</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
	</portlet>
	<portlet>
		<portlet-name>10</portlet-name>
		<friendly-url-mapper-class>com.liferay.wol.groupusersactivities.portlet.GroupUsersActivitiesFriendlyURLMapper</friendly-url-mapper-class>
		<css-class-wrapper>wol-portlet-group-users-activities</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>