<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<icon>/icon.png</icon>
		<struts-path>o/${Bundle-SymbolicName}</struts-path>
		<control-panel-entry-category>apps</control-panel-entry-category>
		<control-panel-entry-weight>23.0</control-panel-entry-weight>
		<control-panel-entry-class>com.liferay.portal.kernel.portlet.OmniadminControlPanelEntry</control-panel-entry-class>
		<use-default-template>false</use-default-template>
		<render-weight>1</render-weight>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>${Bundle-SymbolicName}</css-class-wrapper>
		<add-default-resource>true</add-default-resource>
		<system>true</system>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
</liferay-portlet-app>