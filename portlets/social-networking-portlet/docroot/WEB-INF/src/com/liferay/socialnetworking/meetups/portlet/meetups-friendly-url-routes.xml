<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">

<routes>
	<route>
		<pattern></pattern>
		<implicit-parameter name="jspPage">/meetups/view.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/entry/{meetupsEntryId:\d+}</pattern>
		<implicit-parameter name="jspPage">/meetups/view_entry.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
	</route>
</routes>