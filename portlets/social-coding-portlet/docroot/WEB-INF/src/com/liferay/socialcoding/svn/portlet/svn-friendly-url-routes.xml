<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">

<routes>
	<route>
		<pattern>/rss/all/plugin/trunk</pattern>
		<implicit-parameter name="all">1</implicit-parameter>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
		<implicit-parameter name="url">/plugin/trunk</implicit-parameter>
	</route>
	<route>
		<pattern>/rss/all/portal/trunk</pattern>
		<implicit-parameter name="all">1</implicit-parameter>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
		<implicit-parameter name="url">/portal/trunk</implicit-parameter>
	</route>
	<route>
		<pattern>/rss/user/plugin/trunk</pattern>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
		<implicit-parameter name="url">/plugin/trunk</implicit-parameter>
	</route>
	<route>
		<pattern>/rss/user/portal/trunk</pattern>
		<implicit-parameter name="p_p_state">exclusive</implicit-parameter>
		<implicit-parameter name="url">/portal/trunk</implicit-parameter>
	</route>
</routes>