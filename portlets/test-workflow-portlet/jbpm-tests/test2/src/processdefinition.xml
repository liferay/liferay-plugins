<?xml version="1.0"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="test2">
	<description>Workflow Definition 2</description>
	<swimlane name="testSwimlane1">
		<assignment actor-id="100" />
	</swimlane>
	<start-state name="JavaNode2-1">
		<transition name="toBranchDecision" to="branchDecision">
			<action class="com.liferay.testworkflow.jbpm.handler.SetVariableActionHandler">
				<key>javaNode21</key>
				<value>success</value>
			</action>
		</transition>
	</start-state>
	<decision name="branchDecision">
		<handler class="com.liferay.testworkflow.jbpm.handler.DecisionActionHandler">
			<key>branchName</key>
		</handler>
		<transition name="default" to="taskNode3" />
		<transition name="branch1" to="taskNode1" />
		<transition name="branch2" to="taskNode2" />
	</decision>
	<task-node name="taskNode1">
		<task name="Task2-1" swimlane="testSwimlane1" />
		<transition name="toEnd" to="end" />
	</task-node>
	<task-node name="taskNode2">
		<task name="Task2-2" swimlane="testSwimlane1" />
		<transition name="toEnd" to="end" />
	</task-node>
	<task-node name="taskNode3">
		<task name="Task2-3" swimlane="testSwimlane1" />
		<transition name="toEnd" to="end" />
	</task-node>
	<end-state name="end" />
</process-definition>