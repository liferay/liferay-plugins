<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>1</portlet-name>
		<display-name>My Subscriptions</display-name>
		<portlet-class>com.liferay.mysubscriptions.portlet.MySubscriptionsPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/view.jsp</value>
		</init-param>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>My Subscriptions</title>
			<short-title>My Subscriptions</short-title>
			<keywords>My Subscriptions</keywords>
		</portlet-info>
	</portlet>
</portlet-app>