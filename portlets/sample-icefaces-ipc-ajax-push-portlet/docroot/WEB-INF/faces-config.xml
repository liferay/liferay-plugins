<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config xmlns="http://java.sun.com/JSF/Configuration">
	<application>
		<message-bundle>messages-override</message-bundle>
		<view-handler>com.icesoft.faces.facelets.D2DFaceletViewHandler</view-handler>
	</application>
	<managed-bean>
		<managed-bean-name>renderManager</managed-bean-name>
		<managed-bean-class>com.icesoft.faces.async.render.RenderManager</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bookingService</managed-bean-name>
		<managed-bean-class>com.liferay.sampleicefacesipc.service.impl.BookingServiceMockImpl</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bookingTypeService</managed-bean-name>
		<managed-bean-class>com.liferay.sampleicefacesipc.service.impl.BookingTypeServiceMockImpl</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listSupport</managed-bean-name>
		<managed-bean-class>com.liferay.sampleicefacesipc.bean.support.ListSupport</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>bookingTypeService</property-name>
			<value>#{bookingTypeService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerService</managed-bean-name>
		<managed-bean-class>com.liferay.sampleicefacesipc.service.impl.CustomerServiceMockImpl</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
		<managed-property>
			<property-name>bookingService</property-name>
			<value>#{bookingService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerListModel</managed-bean-name>
		<managed-bean-class>com.liferay.sampleicefacesipc.bean.model.CustomerListModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>customerService</property-name>
			<value>#{customerService}</value>
		</managed-property>
		<managed-property>
			<property-name>renderManager</property-name>
			<value>#{renderManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>customerList</managed-bean-name>
		<managed-bean-class>com.liferay.sampleicefacesipc.bean.backing.CustomerList</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>customerListModel</property-name>
			<value>#{customerListModel}</value>
		</managed-property>
	</managed-bean>
</faces-config>