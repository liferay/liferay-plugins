<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">


<routes>
	<route>
		<pattern>/view</pattern>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
		<implicit-parameter name="jspPage">/contacts_center/view_user.jsp</implicit-parameter>
		<implicit-parameter name="p_p_id">1_WAR_contactsportlet</implicit-parameter>
	</route>

	<route>
		<pattern>/search</pattern>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="jspPage">/contacts_center/view.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">maximized</implicit-parameter>
		<implicit-parameter name="p_p_id">1_WAR_contactsportlet</implicit-parameter>
	</route>

	<route>
		<pattern>/view</pattern>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="jspPage">/contacts_center/view_user.jsp</implicit-parameter>
		<implicit-parameter name="p_p_id">1_WAR_contactsportlet</implicit-parameter>
	</route>

	<route>
		<pattern>/search</pattern>
		<implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
		<implicit-parameter name="jspPage">/contacts_center/view.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
		<implicit-parameter name="p_p_id">1_WAR_contactsportlet</implicit-parameter>
	</route>

</routes>


				