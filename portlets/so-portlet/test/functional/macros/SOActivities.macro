	<command name="viewTasksActivityEntry">
		<if>
			<isset var="row" />
			<then>
				<var name="row" value="${row}" />
			</then>
			<else>
				<var name="row" value="01" />
			</else>
		</if>

		<var name="key_row" value="${row}" />
		<var name="key_userName" value="${userName}" />

		<execute function="AssertElementPresent" locator1="SOActivities#ACTIVITIES_USER_PICTURE" />

		<execute macro="SOActivities#viewActivityEntryUser">
			<var name="dashboardView" value="${dashboardView}" />
			<var name="numberOfOtherCommenters" value="${numberOfOtherCommenters}" />
			<var name="profileView" value="${profileView}" />
			<var name="siteName" value="${siteName}" />
			<var name="userName" value="${userName}" />
		</execute>

		<execute function="AssertElementPresent" locator1="SOActivities#ACTIVITIES_TIME_STAMP" />
		<execute function="AssertTextEquals" locator1="SOActivities#ACTIVITIES_ACTION" value1="${action}" />
		<execute function="AssertTextEquals" locator1="SOActivities#ACTIVITIES_TITLE" value1="${taskDescription}" />
		<execute function="AssertTextEquals#assertPartialText" locator1="SOActivities#ACTIVITIES_CONTENT" value1="Assigned to: ${taskAssignee}" />

		<if>
			<equals arg1="${dueDate}" arg2="true" />
			<then>
				<execute function="AssertTextEquals#assertPartialText" locator1="SOActivities#ACTIVITIES_CONTENT" value1="Due Date: 3/16/16" />
			</then>
			<elseif>
				<equals arg1="${dueDateEdit}" arg2="true" />
				<then>
					<execute function="AssertTextEquals#assertPartialText" locator1="SOActivities#ACTIVITIES_CONTENT" value1="Due Date: 4/17/17" />
				</then>
			</elseif>
			<else>
				<execute function="AssertTextEquals#assertPartialText" locator1="SOActivities#ACTIVITIES_CONTENT" value1="Due Date: None" />
			</else>
		</if>

		<if>
			<isset var="commentCount" />
			<then>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute function="AssertClick" locator1="SOActivities#ACTIVITIES_COMMENT" value1="1 Comment" />
					</then>
					<else>
						<execute function="AssertClick" locator1="SOActivities#ACTIVITIES_COMMENT" value1="${commentCount} Comments" />
					</else>
				</if>
			</then>
			<else>
				<execute function="AssertTextEquals" locator1="SOActivities#ACTIVITIES_COMMENT" value1="Comment" />
			</else>
		</if>

		<execute function="AssertElementPresent" locator1="SOActivities#ACTIVITIES_COMMENT_FIELD" />
	</command>

	<command name="viewWikiActivityEntry">
