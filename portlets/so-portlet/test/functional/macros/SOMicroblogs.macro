<definition>
	<command name="addMicroblogsEntry">
		<if>
			<condition function="IsElementNotPresent" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_GENERAL" />
			<then>
				<execute function="AssertTextEquals" locator1="SOMicroblogs#INFO_MESSAGE" value1="You do not have any microblog entries." />
			</then>
		</if>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
		<execute function="SelectFrameTop" />

		<execute macro="SOMicroblogs#selectViewableByCmd">
			<var name="editInDashboard" value="${editInDashboard}" />
			<var name="microblogsEntryViewableBy" value="${microblogsEntryViewableBy}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
		<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryContent}" />

		<execute macro="SOMicroblogs#viewCharacterCountdownCmd" />

		<execute function="Click" locator1="SOMicroblogs#POST_BUTTON" />

		<if>
			<isset var="microblogsEntryPartialContent" />
			<then>
				<var name="key_microblogsEntryContent" value="${microblogsEntryPartialContent}" />
			</then>
			<else>
				<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />
			</else>
		</if>

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContent}" />
	</command>

	<command name="addMicroblogsEntryComment">
		<execute macro="SOMicroblogs#viewMicroblogsEntryAuthor">
			<var name="microblogsEntryAuthorName" value="${microblogsEntryAuthorName}" />
			<var name="microblogsEntryContent" value="${microblogsEntryContent}" />
			<var name="microblogsEntryPartialContent" value="${microblogsEntryPartialContent}" />
			<var name="microblogsEntryRepostAuthorName" value="${microblogsEntryRepostAuthorName}" />
		</execute>

		<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContent}" />

		<if>
			<condition function="IsElementPresent" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
			<then>
				<execute macro="SOMicroblogs#displayMicroblogsCommentTextFieldCmd">
					<var name="commentCount" value="${commentCount}" />
					<var name="microblogsEntryContent" value="${microblogsEntryContent}" />
				</execute>

				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT_FIELD" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
				<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryCommentContent}" />

				<execute macro="SOMicroblogs#viewCharacterCountdownCmd">
					<var name="characterCountdown" value="${characterCountdown}" />
					<var name="commentInDashboard" value="true" />
					<var name="editInDashboard" value="${editInDashboard}" />
					<var name="textFieldContent?length()" value="${textFieldContent?length()}" />
					<var name="textFieldContentLength" value="${textFieldContentLength}" />
				</execute>

				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_POST_BUTTON" />

				<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />

				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT" value1="${microblogsEntryCommentContent}" />
			</then>
			<else>
				<execute macro="SOMicroblogs#displayMicroblogsCommentTextFieldCmd">
					<var name="commentCount" value="${commentCount}" />
					<var name="microblogsEntryContent" value="${microblogsEntryContent}" />
				</execute>

				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
				<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryCommentContent}" />

				<execute macro="SOMicroblogs#viewCharacterCountdownCmd" />

				<execute function="Click" locator1="SOMicroblogs#POST_BUTTON" />

				<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />

				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT" value1="${microblogsEntryCommentContent}" />
			</else>
		</if>
	</command>

	<command name="addMicroblogsEntryCommentMention">
		<execute macro="SOMicroblogs#viewMicroblogsEntryAuthor">
			<var name="microblogsEntryAuthorName" value="${microblogsEntryAuthorName}" />
			<var name="microblogsEntryContent" value="${microblogsEntryContent}" />
			<var name="microblogsEntryPartialContent" value="${microblogsEntryPartialContent}" />
			<var name="microblogsEntryRepostAuthorName" value="${microblogsEntryRepostAuthorName}" />
		</execute>

		<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContent}" />

		<execute macro="SOMicroblogs#displayMicroblogsCommentTextFieldCmd">
			<var name="commentCount" value="${commentCount}" />
			<var name="microblogsEntryContent" value="${microblogsEntryContent}" />
		</execute>

		<if>
			<condition function="IsElementPresent" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT_FIELD" />
			<then>
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT_FIELD" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
				<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryCommentContent} @${mentionUserFirstName}" />

				<execute macro="SOMicroblogs#viewCharacterCountdownCmd">
					<var name="characterCountdown" value="${characterCountdown}" />
					<var name="commentInDashboard" value="true" />
					<var name="editInDashboard" value="${editInDashboard}" />
					<var name="textFieldContent?length()" value="${textFieldContent?length()}" />
					<var name="textFieldContentLength" value="${textFieldContentLength}" />
				</execute>

				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_EMAIL" value1="${mentionUserEmailAddress}" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserEmailAddress}" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_POST_BUTTON" />

				<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />

				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT" value1="${microblogsEntryCommentContent} ${mentionUserName}" />
			</then>
			<else>
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
				<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryCommentContent} @${mentionUserFirstName}" />

				<execute macro="SOMicroblogs#viewCharacterCountdownCmd" />

				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_EMAIL" value1="${mentionUserEmailAddress}" />
				<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserEmailAddress}" />
				<execute function="Click" locator1="SOMicroblogs#POST_BUTTON" />

				<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />

				<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_CONTENT" value1="${microblogsEntryCommentContent} ${mentionUserName}" />
			</else>
		</if>
	</command>

	<command name="addMicroblogsEntryCommentViaProfile">
		<execute macro="SOPage#gotoPG">
			<var name="defaultURL" value="${defaultURL}" />
			<var name="nodePort" value="${nodePort}" />
			<var name="pageName" value="Contacts Center" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute function="Type#typePause" locator1="SOContactscenter#CONTACTS_LIST_SEARCH" value1="${microblogsEntryAuthorScreenName}" />
		<execute function="AssertClick#pauseAssertTextClickAt" locator1="SOContactscenter#CONTACTS_LIST_NAME" value1="${microblogsEntryAuthorLastName}, ${microblogsEntryAuthorFirstName}" />
		<execute function="AssertClick" locator1="SOContactscenter#USER_DETAILS_NAME" value1="${microblogsEntryAuthorName}" />

		<execute function="Click" locator1="SOProfile#NAVIGATION_MICROBLOGS" value1="Microblogs" />

		<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContent}" />

		<execute macro="SOMicroblogs#displayMicroblogsCommentTextFieldCmd">
			<var name="commentCount" value="${commentCount}" />
			<var name="microblogsEntryContent" value="${microblogsEntryContent}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_CONTENT_FIELD" />
		<execute function="Click" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_CONTENT_INPUT_FIELD" />
		<execute function="Type#sendKeys" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_CONTENT_INPUT_FIELD" value1="${microblogsEntryCommentContent}" />

		<execute macro="SOMicroblogs#viewCharacterCountdownCmd" />

		<execute function="Click" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_POST_BUTTON" />

		<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_CONTENT" value1="${microblogsEntryCommentContent}" />
	</command>

	<command name="addMicroblogsEntryInvalid">
		<if>
			<condition function="IsElementNotPresent" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_GENERAL" />
			<then>
				<execute function="AssertTextEquals" locator1="SOMicroblogs#INFO_MESSAGE" value1="You do not have any microblog entries." />
			</then>
		</if>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
		<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryContent}" />

		<execute macro="SOMicroblogs#viewCharacterCountdownCmd" />

		<execute function="Click" locator1="SOMicroblogs#POST_BUTTON" />
		<execute function="Pause" value1="1000" />

		<execute macro="SOPage#gotoPG">
			<var name="defaultURL" value="${defaultURL}" />
			<var name="nodePort" value="${nodePort}" />
			<var name="pageName" value="Microblogs" />
			<var name="soDashboard" value="true" />
			<var name="soPage" value="true" />
		</execute>

		<execute function="AssertTextNotPresent" value1="${microblogsEntryContent}" />
	</command>

	<command name="addMicroblogsEntryMention">
		<if>
			<condition function="IsElementNotPresent" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_GENERAL" />
			<then>
				<execute function="AssertTextEquals" locator1="SOMicroblogs#INFO_MESSAGE" value1="You do not have any microblog entries." />
			</then>
		</if>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
		<execute function="SelectFrameTop" />

		<execute macro="SOMicroblogs#selectViewableByCmd">
			<var name="editInDashboard" value="${editInDashboard}" />
			<var name="microblogsEntryViewableBy" value="${microblogsEntryViewableBy}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />

		<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryContent} @${mentionUserFirstName}" />

		<execute macro="SOMicroblogs#viewCharacterCountdownCmd" />

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_EMAIL" value1="${mentionUserEmailAddress}" />
		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
		<execute function="Click" locator1="SOMicroblogs#POST_BUTTON" />

		<var name="key_microblogsEntryContent" value="${microblogsEntryContent} ${mentionUserName}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContent} ${mentionUserName}" />
	</command>

	<command name="addMicroblogsEntryMentions">
		<if>
			<condition function="IsElementNotPresent" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_GENERAL" />
			<then>
				<execute function="AssertTextEquals" locator1="SOMicroblogs#INFO_MESSAGE" value1="You do not have any microblog entries." />
			</then>
		</if>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_FIELD" />
		<execute function="SelectFrameTop" />

		<execute macro="SOMicroblogs#selectViewableByCmd">
			<var name="editInDashboard" value="${editInDashboard}" />
			<var name="microblogsEntryViewableBy" value="${microblogsEntryViewableBy}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />
		<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryContent} " />

		<for list="${mentionUserNameList}" param="mentionUserName">
			<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1=" @${mentionUserName}" />
			<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
			<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1=" " />
		</for>

		<execute function="Click" locator1="SOMicroblogs#POST_BUTTON" />
	</command>

	<command name="deleteAndEditMicroblogsEntry">
		<execute function="MakeVisible" locator1="SOMicroblogs#MICROBLOGS_ENTRY_EDIT" />
		<execute function="Click#makeVisibleClickAt" locator1="SOMicroblogs#MICROBLOGS_ENTRY_EDIT_LINK" />
		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_EDIT_FIELD" />
		<execute function="SelectFrameTop" />

		<execute macro="SOMicroblogs#selectViewableByCmd">
			<var name="editInDashboard" value="true" />
			<var name="microblogsEntryViewableBy" value="${microblogsEntryViewableBy}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />

		<execute macro="SOMicroblogs#deleteMicroblogsTextFieldCmd" />

		<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryContentEdit}" />

		<execute macro="SOMicroblogs#viewCharacterCountdownCmd">
			<var name="characterCountdown" value="${characterCountdown}" />
			<var name="editInDashboard" value="true" />
			<var name="textFieldContent?length()" value="${textFieldContent?length()}" />
			<var name="textFieldContentLength" value="${textFieldContentLength}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_EDIT_POST_BUTTON" />

		<var name="key_microblogsEntryContent" value="${microblogsEntryContentEdit}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContentEdit}" />
	</command>

	<command name="deleteAndEditMicroblogsEntryMention">
		<execute function="MakeVisible" locator1="SOMicroblogs#MICROBLOGS_ENTRY_EDIT" />
		<execute function="Click#makeVisibleClickAt" locator1="SOMicroblogs#MICROBLOGS_ENTRY_EDIT_LINK" />
		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_EDIT_FIELD" />
		<execute function="SelectFrameTop" />

		<execute macro="SOMicroblogs#selectViewableByCmd">
			<var name="editInDashboard" value="true" />
			<var name="microblogsEntryViewableBy" value="${microblogsEntryViewableBy}" />
		</execute>

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" />

		<execute macro="SOMicroblogs#deleteMicroblogsTextFieldCmd" />

		<execute function="Type#sendKeys" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="${microblogsEntryContentEdit} @${mentionUserFirstName}" />

		<execute macro="SOMicroblogs#viewCharacterCountdownCmd">
			<var name="characterCountdown" value="${characterCountdown}" />
			<var name="editInDashboard" value="true" />
			<var name="textFieldContent?length()" value="${textFieldContent?length()}" />
			<var name="textFieldContentLength" value="${textFieldContentLength}" />
		</execute>

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_EMAIL" value1="${mentionUserEmailAddress}" />
		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_CONTENT_AUTOCOMPLETE_USERNAME" value1="${mentionUserName}" />
		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_EDIT_POST_BUTTON" />

		<var name="key_microblogsEntryContent" value="${microblogsEntryContentEdit} ${mentionUserName}" />

		<execute function="AssertTextEquals" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT" value1="${microblogsEntryContentEdit} ${mentionUserName}" />
	</command>

	<command name="deleteMicroblogsEntry">
		<execute function="MakeVisible" locator1="SOMicroblogs#MICROBLOGS_ENTRY_DELETE" />
		<execute function="ClickNoError#makeVisibleClick" locator1="SOMicroblogs#MICROBLOGS_ENTRY_DELETE_LINK" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this post?" />
	</command>

	<command name="deleteMicroblogsEntryComment">
		<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />
		<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />

		<if>
			<isset var="commentCount" />
			<then>
				<execute function="AssertClick" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_LINK" value1="${commentCount} Comments" />
			</then>
			<else>
				<execute function="AssertClick" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_LINK" value1="1 Comment" />
			</else>
		</if>

		<execute function="MakeVisible" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_DELETE" />
		<execute function="ClickNoError" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_DELETE_LINK" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this post?" />
	</command>

	<command name="deleteMicroblogsEntryCommentViaProfile">
		<var name="key_microblogsEntryCommentContent" value="${microblogsEntryCommentContent}" />
		<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />

		<execute function="Click" locator1="Dockbar#USER_NAME" />
		<execute function="Click" locator1="Dockbar#USER_MY_PROFILE" value1="My Profile" />

		<execute function="Click" locator1="SOProfile#NAVIGATION_MICROBLOGS" value1="Microblogs" />

		<if>
			<isset var="commentCount" />
			<then>
				<execute function="AssertClick" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_LINK" value1="${commentCount} Comments" />
			</then>
			<else>
				<execute function="AssertClick" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_LINK" value1="1 Comment" />
			</else>
		</if>

		<execute function="MakeVisible" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_DELETE" />
		<execute function="ClickNoError" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_COMMENT_DELETE_LINK" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this post?" />
	</command>

	<command name="deleteMicroblogsEntryViaProfile">
		<execute function="Click" locator1="Dockbar#USER_NAME" />
		<execute function="Click" locator1="Dockbar#USER_MY_PROFILE" value1="My Profile" />

		<execute function="Click" locator1="SOProfile#NAVIGATION_MICROBLOGS" value1="Microblogs" />

		<execute function="MakeVisible" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_DELETE" />
		<execute function="ClickNoError#makeVisibleClick" locator1="SOMicroblogsProfile#MICROBLOGS_ENTRY_DELETE_LINK" value1="Delete" />
		<execute function="Confirm" value1="Are you sure you want to delete this post?" />
	</command>

	<command name="deleteMicroblogsTextFieldCmd">
		<while>
			<condition function="IsElementNotPresent" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_EDIT_COUNTDOWN_WARNING" />
			<then>
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
				<execute function="KeyPress" locator1="SOMicroblogs#MICROBLOGS_CONTENT_INPUT_FIELD" value1="\BACK_SPACE" />
			</then>
		</while>
	</command>

	<command name="displayMicroblogsCommentTextFieldCmd">
		<var name="key_microblogsEntryContent" value="${microblogsEntryContent}" />

		<if>
			<not>
				<contains string="${commentCount}" substring="{commentCount}" />
			</not>
			<then>
				<if>
					<equals arg1="${commentCount}" arg2="1" />
					<then>
						<execute function="AssertClick" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_LINK" value1="1 Comment" />
					</then>
					<else>
						<execute function="AssertClick" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_LINK" value1="${commentCount} Comments" />
					</else>
				</if>
			</then>
			<else>
				<execute function="AssertClick" locator1="SOMicroblogs#MICROBLOGS_ENTRY_COMMENT_LINK" value1="Comment" />
			</else>
		</if>
	</command>

	<command name="editMicroblogsEntry">
		<execute function="MakeVisible" locator1="SOMicroblogs#MICROBLOGS_ENTRY_EDIT" />
		<execute function="Click#makeVisibleClickAt" locator1="SOMicroblogs#MICROBLOGS_ENTRY_EDIT_LINK" />

		<execute function="Click" locator1="SOMicroblogs#MICROBLOGS_ENTRY_CONTENT_EDIT_FIELD" />
		<execute function="SelectFrameTop" />

		<execute macro="SOMicroblogs#selectViewableByCmd">
			<var name="editInDashboard" value="true" />
