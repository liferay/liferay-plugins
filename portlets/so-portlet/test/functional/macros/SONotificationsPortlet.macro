		<var name="key_notificationType" value="Email" />

		<execute function="Check" locator1="PGNotificationsdelivery#NOTIFICATION_DELIVERY_CHECKBOX" />

		<var name="key_contentType" value="Social Office Announcements" />
		<var name="key_notificationType" value="Website" />

		<execute function="Check" locator1="PGNotificationsdelivery#NOTIFICATION_DELIVERY_CHECKBOX" />

		<var name="key_actionType" value="assigns" />
