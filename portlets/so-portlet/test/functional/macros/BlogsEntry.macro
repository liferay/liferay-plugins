<definition>
	<command name="_saveOrPublish">
		<if>
			<equals arg1="${saveAsDraft}" arg2="true" />
			<then>
				<execute function="AssertClick" locator1="PGBlogsEdit#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />

				<execute function="AssertTextEquals" locator1="PGBlogsEdit#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</then>
			<else>
				<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="PGBlogsEdit#PUBLISH_BUTTON" value1="Publish" />

				<execute function="AssertTextEquals" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />
			</else>
		</if>
	</command>

	<command name="addAbstractDescriptionPG">
		<if>
			<condition function="IsElementPresent" locator1="PGBlogsAdd#ABSTRACT_DROPDOWN_COLLAPSED" />
			<then>
				<execute function="AssertClick" locator1="PGBlogsAdd#ABSTRACT_DROPDOWN" value1="Abstract" />
			</then>
		</if>

		<execute function="Type" locator1="PGBlogsAdd#ABSTRACT_DESCRIPTION_FIELD" value1="${entryAbstractDescription}" />

		<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="PGBlogsAdd#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addCategoryCP">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="CPBlogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />

		<execute function="AssertClick" locator1="CPBlogs#ENTRY_TABLE_ACTIONS" value1="Actions" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ENTRY_MENULIST_EDIT" value1="Edit" />

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertElementPresent" locator1="CPBlogsEdit#CONTENT_FIELD" />

		<if>
			<condition function="IsElementNotPresent#isNotVisible" locator1="CPBlogsEdit#CATEGORIZATION_TAGS_ADD_BUTTON" />
			<then>
				<execute function="AssertClick" locator1="CPBlogsEdit#CATEGORIZATION_TOGGLE" value1="Categorization" />

				<execute function="AssertElementPresent" locator1="CPBlogsEdit#CATEGORIZATION_TAGS_ADD_BUTTON" />
			</then>
		</if>

		<execute function="AssertClick" locator1="CPBlogsEdit#CATEGORIZATION_SELECT_BUTTON" value1="Select" />

		<execute function="AssertClick" locator1="CPBlogsEdit#CATEGORIES_CATEGORY_UNSELECTED" value1="${categoryName}" />

		<execute function="AssertElementPresent" locator1="CPBlogsEdit#CATEGORIES_CATEGORY_SELECTED" value1="${categoryName}" />

		<execute function="Click" locator1="CPBlogsEdit#CATEGORIES_CLOSE_BUTTON" />

		<execute function="AssertTextEquals" locator1="CPBlogsEdit#CATEGORIZATION_CATEGORY" value1="${categoryName}" />

		<execute function="AssertClick" locator1="CPBlogsEdit#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />

		<execute function="AssertTextEquals#assertPartialText" locator1="CPBlogsEntry#ENTRY_VOCABULARY" value1="${vocabularyName}" />
		<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="addCategoryPG">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="AssertElementPresent" locator1="PGBlogsAdd#CONTENT_FIELD" />

		<if>
			<condition function="IsElementNotPresent" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_ADD_BUTTON" />
			<then>
				<execute function="AssertClick" locator1="PGBlogsAdd#CATEGORIZATION_TOGGLE" value1="Categorization" />

				<execute function="AssertElementPresent" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_ADD_BUTTON" />
			</then>
		</if>

		<var name="key_categoryName" value="${categoryName}" />
		<var name="key_vocabularyName" value="${vocabularyName}" />

		<execute function="AssertClick" locator1="PGBlogsAdd#CATEGORIZATION_SELECT_BUTTON" value1="Select" />

		<execute function="AssertElementPresent" locator1="PGBlogsAdd#SELECT_CATEGORIES_CATEGORY" />

		<execute function="AssertClick" locator1="PGBlogsAdd#SELECT_CATEGORIES_CATEGORY" value1="${categoryName}" />

		<execute function="AssertElementPresent" locator1="PGBlogsAdd#SELECT_CATEGORIES_CATEGORY_CHECKED" />

		<execute function="Click" locator1="PGBlogsAdd#SELECT_CATEGORIES_CLOSE_BUTTON" />

		<execute function="AssertTextEquals" locator1="PGBlogsAdd#CATEGORIZATION_CATEGORY" value1="${categoryName}" />
	</command>

	<command name="addCP">
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ADD_ENTRY" value1="Add" />

		<execute function="Type" locator1="CPBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CPBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="AssertClick#assertTextClickAtCKEditor" locator1="CPBlogsAdd#PUBLISH_BUTTON" value1="Publish" />

		<var name="key_entryTitle" value="${entryTitle}" />

		<if>
			<equals arg1="${entryInvalidTitle}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="CPBlogsAdd#FIELD_REQUIRED" value1="This field is required." />
			</then>
			<elseif>
				<equals arg1="${entryInvalidContent}" arg2="true" />
				<then>
					<execute function="AssertTextEquals" locator1="CPBlogsAdd#ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute function="AssertTextEquals" locator1="CPBlogsAdd#ERROR_MESSAGE_2" value1="Please enter valid content." />
					<execute function="AssertElementNotPresent" locator1="CPBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				</then>
			</elseif>
			<else>
				<execute function="AssertTextEquals" locator1="CPBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />

				<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_TITLE_HEADER" value1="${entryTitle}" />
				<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_CONTENT" value1="${entryContent}" />
			</else>
		</if>
	</command>

	<command name="addDraftAutoCP">
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ADD_ENTRY" value1="Add" />

		<execute function="Type#typeWaitForCKEditor" locator1="CPBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CPBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="Pause" locator1="30000" />

		<execute function="AssertTextEquals#assertPartialText" locator1="CPBlogsAdd#SAVE_STATUS" value1="Draft saved at" />

		<execute function="AssertTextEquals" locator1="CPBlogsEdit#TITLE_FIELD" value1="${entryTitle}" />

		<execute function="SelectFrame" locator1="CPBlogsEdit#CONTENT_FIELD_IFRAME" />

		<execute function="AssertTextPresent" locator1="CPBlogsEdit#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addDraftAutoPG">
		<execute function="AssertClick" locator1="PGBlogs#ADD_ENTRY_BUTTON" value1="Add Blog Entry" />

		<execute function="Type" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />

		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="PGBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="Pause" locator1="30000" />

		<execute function="AssertTextEquals#assertPartialText" locator1="PGBlogsAdd#SAVE_STATUS" value1="Draft saved at" />

		<execute function="AssertTextEquals" locator1="PGBlogsEdit#TITLE_FIELD" value1="${entryTitle}" />

		<execute function="SelectFrame" locator1="PGBlogsEdit#CONTENT_FIELD_IFRAME" />

		<execute function="AssertTextPresent" locator1="PGBlogsEdit#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addDraftCP">
		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ADD_ENTRY" value1="Add" />

		<execute function="Type#typeWaitForCKEditor" locator1="CPBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />

		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CPBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="CPBlogsAdd#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />

		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="CPBlogsAdd#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="AssertTextEquals" locator1="CPBlogsEdit#TITLE_FIELD" value1="${entryTitle}" />

		<execute function="SelectFrame" locator1="CPBlogsEdit#CONTENT_FIELD_IFRAME" />

		<execute function="AssertTextPresent" locator1="CPBlogsEdit#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addDraftPG">
		<execute function="AssertClick" locator1="PGBlogs#ADD_ENTRY_BUTTON" value1="Add Blog Entry" />

		<execute function="Type" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="PGBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="AssertClick" locator1="PGBlogsAdd#SAVE_AS_DRAFT_BUTTON" value1="Save as Draft" />

		<execute function="AssertTextEquals" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="AssertTextEquals" locator1="PGBlogsEdit#DRAFT_STATUS" value1="Draft" />
		<execute function="AssertTextEquals" locator1="PGBlogsEdit#TITLE_FIELD" value1="${entryTitle}" />

		<execute function="SelectFrame" locator1="PGBlogsEdit#CONTENT_FIELD_IFRAME" />

		<execute function="AssertTextPresent" locator1="PGBlogsEdit#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="SelectFrame" value1="relative=top" />
	</command>

	<command name="addNoPermissionPG">
		<execute function="AssertElementNotPresent" locator1="PGBlogs#ADD_ENTRY_BUTTON" />
	</command>

	<command name="addPG">
		<execute function="AssertClick" locator1="PGBlogs#ADD_ENTRY_BUTTON" value1="Add Blog Entry" />

		<execute function="Type" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="PGBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<if>
			<equals arg1="${entryAddPermissions}" arg2="true" />
			<then>
				<execute macro="BlogsEntry#entryAddPermissionsCmd" />
			</then>
		</if>

		<if>
			<equals arg1="${entryAddCategory}" arg2="true" />
			<then>
				<execute macro="BlogsEntry#addCategoryPG">
					<var name="categoryName" value="${categoryName}" />
					<var name="entryTitle" value="${entryTitle}" />
					<var name="vocabularyName" value="${vocabularyName}" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="${entryAddTag}" arg2="true" />
			<then>
				<execute macro="BlogsEntry#addTagPG">
					<var name="entryTitle" value="${entryTitle}" />
					<var name="multipleTags" value="${multipleTags}" />
					<var name="tagName" value="${tagName}" />
					<var name="tagNameList" value="${tagNameList}" />
				</execute>
			</then>
		</if>

		<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="PGBlogsAdd#PUBLISH_BUTTON" value1="Publish" />

		<if>
			<equals arg1="${entryInvalidTitle}" arg2="true" />
			<then>
				<execute function="AssertTextEquals" locator1="PGBlogsAdd#TITLE_REQUIRED" value1="(Required)" />
				<execute function="AssertTextEquals" locator1="PGBlogsAdd#FIELD_REQUIRED" value1="This field is required." />
			</then>
			<elseif>
				<equals arg1="${entryInvalidContent}" arg2="true" />
				<then>
					<execute function="AssertTextEquals" locator1="PGBlogsAdd#ERROR_MESSAGE_1" value1="Your request failed to complete." />
					<execute function="AssertTextEquals" locator1="PGBlogsAdd#ERROR_MESSAGE_2" value1="Please enter valid content." />
					<execute function="AssertElementNotPresent" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />
				</then>
			</elseif>
			<elseif>
				<equals arg1="${entryInvalidTitleLength}" arg2="true" />
				<then>
					<execute function="AssertTextEquals" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />

					<var name="key_entryTitle" value="${entryValidTitleLength}" />

					<execute function="AssertTextEquals" locator1="PGBlogs#ENTRY_TITLE" value1="${entryValidTitleLength}" />
					<execute function="AssertTextEquals" locator1="PGBlogs#ENTRY_CONTENT" value1="${entryContent}" />
					<execute function="AssertTextNotEquals" locator1="PGBlogs#ENTRY_TITLE" value1="${entryTitle}" />

					<var name="key_entryTitle" value="${entryTitle}" />

					<execute function="AssertElementNotPresent" locator1="PGBlogs#ENTRY_TITLE" />
					<execute function="AssertElementNotPresent" locator1="PGBlogs#ENTRY_CONTENT" />
				</then>
			</elseif>
			<else>
				<execute function="AssertTextEquals" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />

				<var name="key_entryTitle" value="${entryTitle}" />

				<execute function="AssertTextEquals" locator1="PGBlogs#ENTRY_TITLE" value1="${entryTitle}" />
				<execute function="AssertTextEquals" locator1="PGBlogs#ENTRY_CONTENT" value1="${entryContent}" />
			</else>
		</if>

		<if>
			<equals arg1="${entryAddCategory}" arg2="true" />
			<then>
				<execute macro="BlogsEntry#viewCategoryPG">
					<var name="categoryName" value="${categoryName}" />
					<var name="entryTitle" value="${entryTitle}" />
					<var name="vocabularyName" value="${vocabularyName}" />
				</execute>
			</then>
		</if>

		<if>
			<equals arg1="${entryAddTag}" arg2="true" />
			<then>
				<execute macro="BlogsEntry#viewTagPG">
					<var name="entryTitle" value="${entryTitle}" />
					<var name="tagName" value="${tagName}" />
					<var name="tagNameList" value="${tagNameList}" />
				</execute>
			</then>
		</if>
	</command>

	<command name="addPGViaAP">
		<if>
			<isset var="siteScope" />
			<then>
				<var name="key_siteScope" value="${siteScope}" />

				<execute function="AssertClick" locator1="PGAssetpublisher#ADD_NEW_DROPDOWN_IN_SCOPED_SITE" value1="Add New in ${siteScope}" />
			</then>
			<else>
				<execute function="AssertClick" locator1="PGAssetpublisher#ADD_NEW_DROPDOWN" value1="Add New" />
			</else>
		</if>

		<execute function="AssertClick" locator1="PGAssetpublisher#ADD_NEW_MENULIST_BLOGS_ENTRY" value1="Blogs Entry" />

		<execute function="SelectFrame" locator1="PGAssetpublisherAddnewblogsentry#BLOGS_ENTRY_IFRAME" />

		<execute function="Type#typeWaitForCKEditor" locator1="PGAssetpublisherAddnewblogsentry#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="PGAssetpublisherAddnewblogsentry#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="AssertClick#assertTextClickAtCKEditor" locator1="PGAssetpublisherAddnewblogsentry#PUBLISH_BUTTON" value1="Publish" />

		<execute function="Pause" locator1="5000" />

		<execute function="SelectFrame" value1="relative=top" />

		<var name="key_assetTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="PGAssetpublisher#ASSET_ABSTRACT_TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="PGAssetpublisher#ASSET_ABSTRACT_SUMMARY" value1="${entryContent}" />
	</command>

	<command name="addScheduledPG">
		<execute function="AssertClick" locator1="PGBlogs#ADD_ENTRY_BUTTON" value1="Add Blog Entry" />

		<execute function="Type" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />

		<var locator="PGBlogsAdd#DISPLAY_DATE_DATE_HOUR" name="displayDateHour" />
		<var name="displayDateHourLength" value="${displayDateHour?length()}" />

		<if>
			<equals arg1="${displayDateHourLength}" arg2="1" />
			<then>
				<var name="displayDateHour" value="0${displayDateHour}" />
			</then>
		</if>

		<var locator="PGBlogsAdd#DISPLAY_DATE_DATE_MINUTE" name="displayDateMinute" />
		<var method="MathUtil#sum('${displayDateMinute}', '3')" name="displayDateMinuteFuture" />
		<var name="displayDateMinuteFutureLength" value="${displayDateMinuteFuture?length()}" />

		<if>
			<equals arg1="${displayDateMinuteFutureLength}" arg2="1" />
			<then>
				<var name="displayDateMinuteFuture" value="0${displayDateMinuteFuture}" />
			</then>
		</if>

		<var locator="PGBlogsAdd#DISPLAY_DATE_DATE_MERIDIAN" name="displayDateMeridian" />
		<var method="StringUtil#replace('${displayDateMeridian}', '0', 'AM')" name="displayDateMeridian" />
		<var method="StringUtil#replace('${displayDateMeridian}', '1', 'PM')" name="displayDateMeridian" />

		<var name="displayDate" value="${displayDateHour}:${displayDateMinuteFuture} ${displayDateMeridian}" />

		<execute function="Type#clickAtSendKeys" locator1="PGBlogsAdd#DISPLAY_DATE_TIME_INPUT" value1="${displayDate}" />

		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="PGBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="PGBlogsAdd#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="PGBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />
	</command>

	<command name="addTagCP">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="CPBlogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />

		<execute function="AssertClick" locator1="CPBlogs#ENTRY_TABLE_ACTIONS" value1="Actions" />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ENTRY_MENULIST_EDIT" value1="Edit" />

		<execute function="AssertElementPresent" locator1="CPBlogsEdit#CONTENT_FIELD" />

		<if>
			<condition function="IsElementNotPresent#isNotVisible" locator1="CPBlogsEdit#CATEGORIZATION_TAGS_ADD_BUTTON" />
			<then>
				<execute function="AssertClick" locator1="CPBlogsEdit#CATEGORIZATION_TOGGLE" value1="Categorization" />

				<execute function="AssertElementPresent" locator1="CPBlogsEdit#CATEGORIZATION_TAGS_ADD_BUTTON" />
			</then>
		</if>

		<var name="key_tagName" value="${tagName}" />

		<execute function="Type" locator1="CPBlogsEdit#CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />

		<execute function="AssertClick" locator1="CPBlogsEdit#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />

		<execute function="AssertTextEquals" locator1="CPBlogsEdit#CATEGORIZATION_TAG" value1="${tagName}" />

		<execute function="AssertClick" locator1="CPBlogsEdit#PUBLISH_BUTTON" value1="Publish" />

		<execute function="AssertTextEquals" locator1="CPBlogs#SUCCESS_MESSAGE" value1="Your request completed successfully." />

		<execute function="AssertClick#assertTextClickAtAndWait" locator1="CPBlogs#ENTRY_TABLE_TITLE" value1="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="CPBlogsEntry#ENTRY_TAG" value1="${tagName}" />
	</command>

	<command name="addTagPG">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="AssertElementPresent" locator1="PGBlogsAdd#CONTENT_FIELD" />

		<if>
			<condition function="IsElementNotPresent" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_ADD_BUTTON" />
			<then>
				<execute function="AssertClick" locator1="PGBlogsAdd#CATEGORIZATION_TOGGLE" value1="Categorization" />

				<execute function="AssertElementPresent" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_ADD_BUTTON" />
			</then>
		</if>

		<if>
			<isset var="tagNameList" />
			<then>
				<for list="${tagNameList}" param="tagName">
					<execute function="Type#clickAtSendKeys" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />

					<execute function="AssertClick" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />
				</for>
			</then>
			<else>
				<var name="key_tagName" value="${tagName}" />

				<execute function="Type#clickAtSendKeys" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_FIELD" value1="${tagName}" />

				<execute function="AssertClick" locator1="PGBlogsAdd#CATEGORIZATION_TAGS_ADD_BUTTON" value1="Add" />

				<execute function="AssertElementPresent" locator1="PGBlogsAdd#CATEGORIZATION_TAG" value1="${tagName}" />
			</else>
		</if>
	</command>

	<command name="addWithTrackbackURLPG">
		<var name="key_entryTitle" value="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="PGBlogs#ENTRY_TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="PGBlogs#ENTRY_CONTENT" value1="${entryContent}" />

		<execute function="AssertClick" locator1="PGBlogs#ENTRY_TITLE" value1="${entryTitle}" />

		<execute function="AssertTextEquals" locator1="PGBlogsEntry#ENTRY_TITLE" value1="${entryTitle}" />
		<execute function="AssertTextEquals" locator1="PGBlogsEntry#ENTRY_CONTENT" value1="${entryContent}" />

		<execute function="Pause" locator1="3000" />

		<execute function="Copy" locator1="PGBlogsEntry#COMMENTS_TRACKBACK_URL" />

		<execute macro="Page#gotoPG">
			<var name="defaultURL" value="${defaultURL}" />
			<var name="nodePort" value="${nodePort}" />
			<var name="pageName" value="${pageName}" />
			<var name="soDashboard" value="${soDashboard}" />
			<var name="soPage" value="${soPage}" />
		</execute>

		<execute function="Pause" locator1="1000" />

		<execute function="AssertClick" locator1="PGBlogs#ADD_ENTRY_BUTTON" value1="Add Blog Entry" />

		<execute function="Type" locator1="PGBlogsAdd#TITLE_FIELD" value1="${entryTitle2}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="PGBlogsAdd#CONTENT_FIELD" value1="${entryContent2}" />

		<execute function="Pause" locator1="3000" />

		<execute function="Paste" locator1="PGBlogsAdd#TRACKBACKS_TO_SEND_FIELD" />

		<execute function="Pause" locator1="3000" />

		<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="PGBlogsAdd#PUBLISH_BUTTON" value1="Publish" />
	</command>

	<command name="addWithWorkflowCP">
		<execute function="Click" locator1="CPBlogs#ADD_ENTRY" />

		<execute function="Type#typeWaitForCKEditor" locator1="CPBlogsAdd#TITLE_FIELD" value1="${entryTitle}" />
		<execute function="Type#typeCKEditorWaitForCKEditor" locator1="CPBlogsAdd#CONTENT_FIELD" value1="${entryContent}" />

		<execute function="AssertClick#assertTextClickAtAndWaitCKEditor" locator1="CPBlogsAdd#SUBMIT_FOR_PUBLICATION_BUTTON" value1="Submit for Publication" />

		<var name="key_entryTitle" value="${entryTitle}" />

