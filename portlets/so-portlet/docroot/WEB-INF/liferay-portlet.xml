<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>1</portlet-name>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>user_activities</friendly-url-mapping>
		<friendly-url-routes>com/liferay/portal/kernel/portlet/rss-friendly-url-routes.xml</friendly-url-routes>
		<header-portlet-css>/activities/css/main.css</header-portlet-css>
		<css-class-wrapper>so-portlet-activities</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>2</portlet-name>
		<header-portlet-css>/invite_members/css.jsp</header-portlet-css>
		<header-portlet-javascript>/invite_members/javascript.js</header-portlet-javascript>
		<css-class-wrapper>so-portlet-invite-members</css-class-wrapper>
		<system>true</system>
	</portlet>
	<portlet>
		<portlet-name>3</portlet-name>
		<css-class-wrapper>so-portlet-members</css-class-wrapper>
		<system>true</system>
	</portlet>
	<portlet>
		<portlet-name>4</portlet-name>
		<friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
		<friendly-url-mapping>profiles</friendly-url-mapping>
		<friendly-url-routes>com/liferay/so/profiles/portlet/profiles-friendly-url-routes.xml</friendly-url-routes>
		<social-request-interpreter-class>com.liferay.so.profiles.social.ProfilesRequestInterpreter</social-request-interpreter-class>
		<header-portlet-javascript>/profiles/javascript.js</header-portlet-javascript>
		<footer-portal-javascript>/html/js/liferay/service.js</footer-portal-javascript>
		<css-class-wrapper>so-portlet-profiles</css-class-wrapper>
		<system>true</system>
	</portlet>
	<portlet>
		<portlet-name>5</portlet-name>
		<header-portlet-javascript>/sites/js/main.js</header-portlet-javascript>
		<footer-portal-javascript>/html/js/liferay/service.js</footer-portal-javascript>
		<css-class-wrapper>so-portlet-sites</css-class-wrapper>
	</portlet>
	<portlet>
		<portlet-name>6</portlet-name>
		<header-portlet-css>/notifications/css/main.css</header-portlet-css>
		<css-class-wrapper>so-portlet-notifications</css-class-wrapper>
		<system>true</system>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>