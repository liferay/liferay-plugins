		</sequence>
	</complexType>
	<complexType name="ClientData">
		<sequence>
			<element name="userAgent" type="xsd:string" minOccurs="0" />
			<element name="extensions" type="types:Extension" minOccurs="0" maxOccurs="unbounded" />
