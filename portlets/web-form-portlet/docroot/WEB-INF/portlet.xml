<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	<portlet>
		<portlet-name>1</portlet-name>
		<display-name>Web Form</display-name>
		<portlet-class>com.liferay.webform.portlet.WebFormPortlet</portlet-class>
		<init-param>
			<name>copy-request-parameters</name>
			<value>true</value>
		</init-param>
		<init-param>
			<name>view-template</name>
			<value>/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>Web Form</title>
			<short-title>Web Form</short-title>
			<keywords>Web Form</keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>description</name>
				<value>Your input is valuable to us. Please send us your suggestions.</value>
			</preference>
			<preference>
				<name>fieldLabel1</name>
				<value>Name</value>
			</preference>
			<preference>
				<name>fieldLabel2</name>
				<value>Rating</value>
			</preference>
			<preference>
				<name>fieldLabel3</name>
				<value>Comments</value>
			</preference>
			<preference>
				<name>fieldOptions2</name>
				<value>Excellent,Good,Satisfactory,Poor</value>
			</preference>
			<preference>
				<name>fieldType1</name>
				<value>text</value>
			</preference>
			<preference>
				<name>fieldType2</name>
				<value>options</value>
			</preference>
			<preference>
				<name>fieldType3</name>
				<value>textarea</value>
			</preference>
			<preference>
				<name>saveToDatabase</name>
				<value>true</value>
			</preference>
			<preference>
				<name>subject</name>
				<value>Suggestions</value>
			</preference>
			<preference>
				<name>title</name>
				<value>Suggestions</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>