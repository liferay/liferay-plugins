<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_0_0.dtd">

<routes>
	<route>
		<pattern></pattern>
		<implicit-parameter name="mvcPath">/view.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>{mvcPath}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<ignored-parameter name="p_p_resource_id" />
	</route>
	<route>
		<pattern>/logo.png</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_resource_id">logo.png</implicit-parameter>
	</route>
</routes>