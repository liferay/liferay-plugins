<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	<portlet>
		<portlet-name>1</portlet-name>
		<display-name>Test Misc</display-name>
		<portlet-class>com.liferay.testmisc.portlet.TestPortlet</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Test Misc</title>
			<short-title>Test Misc</short-title>
			<keywords>Test Misc</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<filter>
		<filter-name>Test Render Filter</filter-name>
		<filter-class>com.liferay.testmisc.portlet.TestRenderFilter</filter-class>
		<lifecycle>RENDER_PHASE</lifecycle>
	</filter>
	<filter-mapping>
		<filter-name>Test Render Filter</filter-name>
		<portlet-name>1</portlet-name>
	</filter-mapping>
</portlet-app>