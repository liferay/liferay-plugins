<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.sharing">
	<namespace>Sharing</namespace>
	<entity name="SharingEntry" local-service="true" remote-service="true">

		<!-- PK fields -->

		<column name="classNameId" type="long" primary="true" />
		<column name="classPK" type="long" primary="true" />
		<column name="sharingClassNameId" type="long" primary="true" />
		<column name="sharingClassPK" type="long" primary="true" />

		<!-- Finder methods -->

		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="S_S" return-type="Collection">
			<finder-column name="sharingClassNameId" />
			<finder-column name="sharingClassPK" />
		</finder>
		<finder name="C_C_S" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
			<finder-column name="sharingClassNameId" />
		</finder>
		<finder name="C_S_S"  return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="sharingClassNameId" />
			<finder-column name="sharingClassPK" />
		</finder>
	</entity>
</service-builder>