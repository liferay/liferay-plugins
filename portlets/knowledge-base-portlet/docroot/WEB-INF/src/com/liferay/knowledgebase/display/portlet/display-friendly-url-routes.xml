<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.2.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_2_0.dtd">

<routes>
	<route>
		<pattern></pattern>
		<implicit-parameter name="mvcPath">/display/view.jsp</implicit-parameter>
	</route>
	<route>
		<pattern>/rss</pattern>
		<ignored-parameter name="mvcPath" />
		<ignored-parameter name="p_p_state" />
		<implicit-parameter name="p_p_cacheability">cacheLevelPage</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">2</implicit-parameter>
		<implicit-parameter name="p_p_resource_id">rss</implicit-parameter>
	</route>
	<route>
		<pattern>/administrator</pattern>
		<implicit-parameter name="mvcPath">/display/view_admin_panel.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/home</pattern>
		<implicit-parameter name="mvcPath">/display/view.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/my-subscriptions</pattern>
		<implicit-parameter name="mvcPath">/display/view_subscribed_articles.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/recent-articles</pattern>
		<implicit-parameter name="mvcPath">/display/view_recent_articles.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/search</pattern>
		<implicit-parameter name="mvcPath">/display/search.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/article/{resourcePrimKey:\d+}</pattern>
		<implicit-parameter name="mvcPath">/display/view_article.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/template/{kbTemplateId:\d+}</pattern>
		<implicit-parameter name="mvcPath">/display/view_template.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
	<route>
		<pattern>/article/{resourcePrimKey:\d+}/{status:-?\d+}/history</pattern>
		<implicit-parameter name="mvcPath">/display/history.jsp</implicit-parameter>
		<implicit-parameter name="p_p_state">normal</implicit-parameter>
	</route>
</routes>