<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="zoe-healthcare" name="Zoe Healthcare">
		<settings>
			<setting configurable="true" key="display-company-logo" type="checkbox" value="false" />
			<setting configurable="true" key="display-footer" type="checkbox" value="true" />
			<setting configurable="true" key="footer-nav-title" type="text" value="Site Map" />
			<setting configurable="true" key="footer-section-1-article-id" type="text" />
			<setting configurable="true" key="footer-section-2-article-id" type="text" />
			<setting configurable="true" key="footer-section-3-article-id" type="text" />
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="false" />
			<setting configurable="false" key="show-site-name-default" type="checkbox" value="false" />
		</settings>
		<layout-templates>
			<custom>
				<layout-template id="1-2-3-columns" name="1-2-3 Columns Layout">
					<template-path>/layouttpl/custom/1-2-3-columns-layouttpl/1_2_3_columns.tpl</template-path>
					<wap-template-path>/layouttpl/custom/1-2-3-columns-layouttpl/1_2_3_columns.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/1-2-3-columns-layouttpl/1_2_3_columns.png</thumbnail-path>
				</layout-template>
				<layout-template id="2-1-2-4-columns" name="2-1-2-4 Columns Layout">
					<template-path>/layouttpl/custom/2-1-2-4-columns-layouttpl/2_1_2_4_columns.tpl</template-path>
					<wap-template-path>/layouttpl/custom/2-1-2-4-columns-layouttpl/2_1_2_4_columns.tpl</wap-template-path>
					<thumbnail-path>/layouttpl/custom/2-1-2-4-columns-layouttpl/2_1_2_4_columns.png</thumbnail-path>
				</layout-template>
			</custom>
		</layout-templates>
	</theme>
</look-and-feel>